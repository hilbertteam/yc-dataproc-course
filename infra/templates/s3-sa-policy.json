{
  "Statement": [
    {
      "Sid": "Allow all for ${S3_SA_NAME}",
      "Effect": "Allow",
      "Principal": {
        "CanonicalUser": [
          "${S3_SA_ID}"
        ]
      },
      "Action": "*",
      "Resource": [
        "arn:aws:s3:::${S3_BUCKET_NAME}/*",
        "arn:aws:s3:::${S3_BUCKET_NAME}"
      ]
    }
  ]
}
